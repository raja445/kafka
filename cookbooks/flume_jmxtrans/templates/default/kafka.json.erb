{
 "servers": [
  {
   "numQueryThreads": 8,
   "alias": "<%= @host %>",
   "host": "<%= @fqdn %>",
   "port": 2520,
   "queries": [
    {
     "resultAlias": "flumekafkasink",
     "obj": "org.apache.flume.sink:type=kafkasink,*",
     "attr": [
     ],
     "outputWriters": [
      {
       "@class": "com.googlecode.jmxtrans.model.output.GraphiteWriter",
       "settings": {
        "typeNames": [
         "name"
        ],
        "host": "<%= @graphite_host %>",
        "port": 2020,
        "rootPrefix": "prod.<%= @colo %>.grid.<%= @cluster_name %>.app"
       }
      }
     ]
    },
    {
     "resultAlias": "flumesource",
     "obj": "org.apache.flume.source:type=avrosrc,*",
     "attr": [
     ],
     "outputWriters": [
      {
       "@class": "com.googlecode.jmxtrans.model.output.GraphiteWriter",
       "settings": {
        "typeNames": [
         "name"
        ],
        "host": "<%= @graphite_host %>",
        "port": 2020,
        "rootPrefix": "prod.<%= @colo %>.grid.<%= @cluster_name %>.app"
       }
      }
     ]
    },
    {
     "resultAlias": "flumechannel",
     "obj": "org.apache.flume.channel:type=spillable,*",
     "attr": [
     ],
     "outputWriters": [
      {
       "@class": "com.googlecode.jmxtrans.model.output.GraphiteWriter",
       "settings": {
        "typeNames": [
         "name"
        ],
        "host": "<%= @graphite_host %>",
        "port": 2020,
        "rootPrefix": "prod.<%= @colo %>.grid.<%= @cluster_name %>.app"
       }
      }
     ]
    }
  ]
 }
 ]
}
